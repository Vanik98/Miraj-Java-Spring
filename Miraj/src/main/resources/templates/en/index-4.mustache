</section>
<div class="form_block">
  <div class="container">
    <div class="row">
      <div class="grid_11">
        <h2>Contact Form</h2>
          <form id="form" method="post" action="/message">
        <div class="form_spinner">
        <img src="images/Preloader_4.gif" alt="">
        </div>
        <div class="modal fade response-message">
        <div class="modal-dialog">
        <div class="modal-content">
        <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
        <h4 class="modal-title">Modal title</h4>
        </div>
        <div class="modal-body">
        </div>      
        </div>
        </div>
        </div>
        <div class="response-error-message"></div>
        <label class="name">
        <input required type="text" name="Name2" placeholder="Name:" value="" data-constraints="@Required @JustLetters"
               pattern="[A-Za-z\s]{2,10}" title="Enter real data"/>
        </label>
        <label class="email">
        <input required type="text" name="email" placeholder="E-mail:" value="" data-constraints="@Required @Email"
                pattern="[a-z0-9._%+-]+@[a-z0-9.-]+\.[a-z]{2,3}$" title="Enter real data"/>
        </label>
        <label class="phone">
        <input required  pattern="[0-9\s]{6,12}"  title="Enter real data" type="text" name="Phone2" placeholder="Phone:" value="" data-constraints="@Required @JustNumbers"/>
        </label>
        <label class="message">
        <textarea required name="message" placeholder="Message:" data-constraints='@Required @Length(min=20,max=999999)'></textarea>
        </label>
              <input id="pagecontacts" name="pagecontacts" type="hidden" value="en"/>
        <div class="clear"></div>
        <div class="btns">
        <a href="#" data-type="reset" class="btn">clear</a>
            <input type="hidden" name="_csrf" value="{{_csrf.token}}" />
            <button data-type="submit" class="btn">send</button>
        </div>
        </form>

      </div>
    </div>
  </div>
</div>
